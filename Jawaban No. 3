#include <stdio.h>
int main() {
    int array[5];
    int *ptr = array;
    int i;

    // Input nilai array
    printf("Masukkan 5 elemen array:\n");
    for (i = 0; i < 5; i++) {
        printf("Elemen ke-%d: ", i + 1);
        scanf("%d", &array[i]);
    }

    // Menampilkan elemen array menggunakan pointer
    printf("\nElemen array: ");
    for (i = 0; i < 5; i++) {
        printf("%d ", *(ptr + i));
    }
    return 0;
}
